{
  "name": "geelong-stars",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "pages:build": "npm run build",
    "pages:dev": "npm run build && wrangler pages dev dist --compatibility-date=2024-01-01 --d1=DB",
    "deploy": "npm run build && wrangler pages deploy dist",
    "db:init": "wrangler d1 execute geelong-stars-db --local --file=./schema.sql"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "@tanstack/react-query": "^5.17.0",
    "react-hook-form": "^7.49.0",
    "lucide-react": "^0.309.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@cloudflare/workers-types": "^4.20240117.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "@vitejs/plugin-react": "^4.2.1",
    "tailwindcss": "^4.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "wrangler": "^3.22.4"
  }
}
